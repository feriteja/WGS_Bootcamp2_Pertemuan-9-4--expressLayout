<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %>  </title>
</head>
<body>
    
    <h1>Home page</h1>
    <nav>
        <ul>
            <li>
                <a href="/contact">Contact</a>
            </li>
            <li>

                <a href="/about">About</a>
            </li>
            <li>

                <a href="/">Home</a>
            </li>
        </ul>
    </nav>

    
    <h2>Web server nodeJS</h2>
    <p>Wellcome to my page <%= name %> </p>
    <ul>
    <%    contacts.forEach(element => { %>
            <li>
                <h3><%= element.name %>  </h3>
                
                <h4><%= element.email %>  </h4>
            </li>
    <% })   %>  
    
    <% if (contacts.length>0) { %>
        <% contacts.forEach(element => { %>
            <li>
                <h3><%= element.name %>  </h3>
                
                <h4><%= element.email %>  </h4>
            </li>
         
        <% }) %>
     
    <% } else { %>
        <p>not found</p>
        <% }%>
     
</ul>
</body>
</html>